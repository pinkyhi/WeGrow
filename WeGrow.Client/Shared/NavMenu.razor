            <MudNavGroup Title="Admin" Expanded="true">
                <MudNavLink Href="/admin/modules"  Match="NavLinkMatch.Prefix">Modules</MudNavLink>
                <MudNavLink Href="/admin/receipts"  Match="NavLinkMatch.Prefix">Receipts</MudNavLink>
